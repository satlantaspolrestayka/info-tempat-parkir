[file content begin]
{
  "name": "ops-ketupat-progo-parking-system",
  "version": "2.0.0",
  "description": "Advanced parking data validation and management system for Operasi Ketupat Progo 2026",
  "main": "scripts/validate-parking.js",
  "scripts": {
    "start": "node scripts/validate-parking.js",
    
    "validate": "node scripts/validate-parking.js",
    "validate:dry-run": "node scripts/validate-parking.js --dry-run",
    "validate:strict": "node scripts/validate-parking.js --mode=strict",
    "validate:fix": "node scripts/validate-parking.js --mode=fix --force",
    "validate:report": "node scripts/validate-parking.js --mode=report-only",
    "validate:quick": "node scripts/validate-parking.js --quick",
    
    "fix:stats": "node scripts/fix-statistics.js",
    "fix:quick": "node scripts/fix-statistics.js --quick",
    
    "verify:consistency": "node scripts/verify-consistency.js",
    "verify:config": "node scripts/verify-consistency.js --config-check",
    
    "process:updates": "node scripts/process-updates.js",
    "validate:updates": "node scripts/validate-updates.js",
    "validate:updates-quick": "node scripts/validate-updates.js --quick",
    "validate:updates-clean": "node scripts/validate-updates.js --clean",
    
    "monitor:stats": "node scripts/monitor-statistics.js",
    "monitor:health": "node scripts/health-check.js",
    
    "backup:create": "node scripts/backup-manager.js --create",
    "backup:list": "node scripts/backup-manager.js --list",
    "backup:restore": "node scripts/backup-manager.js --restore",
    
    "notify:utilization": "node scripts/notify-utilization.js",
    "audit:logs": "node scripts/audit-logger.js --report",
    
    "sync:config": "node scripts/sync-config.js",
    "emergency:recover": "node scripts/emergency-recovery.js",
    "deploy:check": "node scripts/deploy-check.js",
    
    "test": "node scripts/test-validator.js",
    "test:data": "node tests/test-data-integrity.js",
    "test:workflows": "node tests/test-workflows.js",
    "test:emergency": "node tests/test-emergency.js",
    
    "setup": "bash scripts/setup-validator.sh",
    "lint": "eslint scripts/*.js",
    "format": "prettier --write scripts/*.js"
  },
  "bin": {
    "parking-validator": "./scripts/validate-parking.js",
    "parking-fixer": "./scripts/fix-statistics.js",
    "parking-verify": "./scripts/verify-consistency.js",
    "parking-updates": "./scripts/process-updates.js"
  },
  "keywords": [
    "parking",
    "validation",
    "statistics",
    "data-quality",
    "operasi-ketupat",
    "yogyakarta",
    "satlantas"
  ],
  "author": "Satlantas Polresta Yogyakarta",
  "license": "MIT",
  "dependencies": {
    "minimist": "^1.2.8",
    "chalk": "^4.1.2",
    "dayjs": "^1.11.9",
    "winston": "^3.11.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/satlantaspolrestayka/ops-ketupat-progo-2026.git"
  },
  "homepage": "https://github.com/satlantaspolrestayka/ops-ketupat-progo-2026#readme",
  "bugs": {
    "url": "https://github.com/satlantaspolrestayka/ops-ketupat-progo-2026/issues"
  }
}
[file content end]
